{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "crds": {
            "type": "object",
            "properties": {
                "deployAlloyCRD": {
                    "type": "boolean"
                },
                "deployPodLogsCRD": {
                    "type": "boolean"
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {
                        "pullSecrets": {
                            "type": "array"
                        },
                        "registry": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "pullSecrets": {
                    "type": "array"
                },
                "registry": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "namespaces": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "nodeSelector": {
            "type": "object",
            "properties": {
                "kubernetes.io/os": {
                    "type": "string"
                }
            }
        },
        "ownNamespaceOnly": {
            "type": "boolean"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "priorityClassName": {
            "type": "string"
        },
        "rbac": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "createClusterRoles": {
                    "type": "boolean"
                }
            }
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                }
            }
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object"
                },
                "requests": {
                    "type": "object"
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "runAsNonRoot": {
                    "type": "boolean"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "health": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "metrics": {
                    "type": "object",
                    "properties": {
                        "port": {
                            "type": "integer"
                        }
                    }
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "automount": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "labels": {
                    "type": "object"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
